{
  "name": "@kit/ui",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "build": "bun run merge-exports && bun run build:components && bun run build:dts:cached",
    "build:vercel": "echo 'Skipping UI build in Vercel environment'",
    "merge-exports": "node scripts/merge-exports.js",
    "build:components": "concurrently \"bun run build:shadcn\" \"bun run build:makerkit\" \"bun run build:magicui\" \"bun run build:utils\" \"bun run build:custom\"",
    "build:shadcn": "tsup --config config/tsup/build/tsup.shadcn.config.ts",
    "build:makerkit": "tsup --config config/tsup/build/tsup.makerkit.config.ts",
    "build:magicui": "tsup --config config/tsup/build/tsup.magicui.config.ts",
    "build:utils": "tsup --config config/tsup/build/tsup.utils.config.ts",
    "build:custom": "tsup --config config/tsup/build/tsup.custom.config.ts",
    "build:dts:all": "bun run build:dts:shadcn && bun run build:dts:makerkit && bun run build:dts:magicui && bun run build:dts:utils && bun run build:dts:custom",
    "build:dts:cached": "bun run build:dts:shadcn:cached && bun run build:dts:makerkit:cached && bun run build:dts:magicui:cached && bun run build:dts:utils:cached && bun run build:dts:custom:cached",
    "build:dts": "NODE_OPTIONS=\"--max-old-space-size=8192\" tsup --config config/tsup/dts/dts.config.ts",
    "build:dts:shadcn": "NODE_OPTIONS=\"--max-old-space-size=8192\" tsup --config config/tsup/dts/dts.shadcn.config.ts",
    "build:dts:makerkit": "NODE_OPTIONS=\"--max-old-space-size=8192\" tsup --config config/tsup/dts/dts.makerkit.config.ts",
    "build:dts:magicui": "NODE_OPTIONS=\"--max-old-space-size=8192\" tsup --config config/tsup/dts/dts.magicui.config.ts",
    "build:dts:utils": "NODE_OPTIONS=\"--max-old-space-size=8192\" tsup --config config/tsup/dts/dts.utils.config.ts",
    "build:dts:custom": "NODE_OPTIONS=\"--max-old-space-size=8192\" tsup --config config/tsup/dts/dts.custom.config.ts",
    "build:dts:shadcn:cached": "node config/scripts/cached-build.js shadcn build:dts:shadcn",
    "build:dts:makerkit:cached": "node config/scripts/cached-build.js makerkit build:dts:makerkit",
    "build:dts:magicui:cached": "node config/scripts/cached-build.js magicui build:dts:magicui",
    "build:dts:utils:cached": "node config/scripts/cached-build.js utils build:dts:utils",
    "build:dts:custom:cached": "node config/scripts/cached-build.js custom build:dts:custom",
    "postinstall": "mkdir -p .cache/tsup-cache",
    "dev": "concurrently \"bun run dev:components\" \"bun run dev:types\"",
    "dev:components": "concurrently \"bun run dev:shadcn:watch\" \"bun run dev:makerkit:watch\" \"bun run dev:magicui:watch\" \"bun run dev:utils:watch\" \"bun run dev:custom:watch\"",
    "dev:types": "bun run build:dts:cached",
    "dev:shadcn:watch": "tsup --config config/tsup/dev/dev.shadcn.config.ts",
    "dev:makerkit:watch": "tsup --config config/tsup/dev/dev.makerkit.config.ts",
    "dev:magicui:watch": "tsup --config config/tsup/dev/dev.magicui.config.ts",
    "dev:utils:watch": "tsup --config config/tsup/dev/dev.utils.config.ts",
    "dev:custom:watch": "tsup --config config/tsup/dev/dev.custom.config.ts",
    "clean": "git clean -xdf .turbo node_modules dist",
    "format": "biome format .",
    "lint": "biome lint .",
    "typecheck": "tsc --noEmit",
    "storybook": "storybook dev -p 6006 --config-dir .storybook",
    "build-storybook": "storybook build --config-dir .storybook"
  },
