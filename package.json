{
  "name": "super-next-app",
  "private": true,
  "version": "0.1.0",
  "sideEffects": false,
  "engines": {
    "node": ">=20.10.0",
    "bun": ">=1.0.0"
  },
  "author": {
    "url": "https://saedgewell.net",
    "name": "Saedgewell"
  },
  "template": {
    "name": "super-next-app",
    "author": {
      "url": "https://saedgewell.net",
      "name": "Saedgewell"
    },
    "repository": "https://github.com/otomatty/super-next-app"
  },
  "scripts": {
    "preinstall": "bun run --cwd tooling/scripts requirements",
    "build": "turbo run build",
    "build:web": "turbo run build --filter=web",
    "build:docs": "turbo run build --filter=docs",
    "build:admin": "turbo run build --filter=admin",
    "build:circle": "turbo run build --filter=circle",
    "build:web:vercel": "bun run build:prepare-vercel && turbo run build:vercel --filter=web",
    "build:docs:vercel": "bun run build:prepare-vercel && turbo run build:vercel --filter=docs",
    "build:admin:vercel": "bun run build:prepare-vercel && turbo run build:vercel --filter=admin",
    "build:circle:vercel": "bun run build:prepare-vercel && turbo run build:vercel --filter=circle",
    "build:prepare-vercel": "mkdir -p packages/ui/dist && touch packages/ui/dist/.gitkeep",
    "build:vercel": "bun run build:packages && bun run build:admin:vercel",
    "clean": "git clean -xdf .turbo node_modules dist",
    "clean:workspaces": "turbo clean",
    "dev": "turbo run dev --parallel --filter=!@kit/ui",
    "dev:ui": "bun run --cwd packages/ui dev",
    "dev:web": "bun run --cwd apps/web dev",
    "dev:docs": "bun run --cwd apps/docs dev",
    "dev:admin": "bun run --cwd apps/admin dev",
    "dev:circle": "bun run --cwd apps/circle dev",
    "dev:all": "concurrently \"bun run dev\" \"bun run dev:ui\"",
    "dev:packages": "concurrently \"bun run dev:ui\"",
    "format": "biome format .",
    "format:fix": "turbo format --cache-dir=.turbo --continue -- biome format . --write",
    "lint": "biome lint .",
    "lint:fix": "turbo lint --cache-dir=.turbo --continue -- biome lint . --apply",
    "typecheck": "tsc --noEmit",
    "test": "turbo test --cache-dir=.turbo",
    "update": "bun update",
    "syncpack:list": "bun x syncpack list-mismatches",
    "syncpack:fix": "bun x syncpack fix-mismatches",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset",
    "supabase:typegen": "bunx supabase gen types typescript --linked --schema public > packages/supabase/src/database.types.ts",
    "supabase:typegen:estimate-agent": "bunx supabase gen types typescript --linked --schema estimate_agent > apps/estimate-agent/src/types/database.types.ts",
    "supabase:typegen:circle": "bunx supabase gen types typescript --linked --schema circle > packages/supabase/src/circle-database.types.ts",
    "supabase:migration:new": "supabase migration new",
    "supabase:migration:up": "supabase migration up",
    "version:check": "bun run tooling/scripts/version-manager/cli.ts check",
    "version:update": "bun run tooling/scripts/version-manager/cli.ts update",
    "version:update:selected": "bun run tooling/scripts/version-manager/cli.ts update:selected",
    "version:update:next": "bun run tooling/scripts/version-manager/cli.ts update next",
    "version:update:react": "bun run tooling/scripts/version-manager/cli.ts update react",
    "version:update:supabase": "bun run tooling/scripts/version-manager/cli.ts update supabase",
    "build:packages": "turbo run build --filter=./packages/*"
  },
  "packageManager": "bun@1.2.4",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@turbo/gen": "^2.4.4",
    "@types/jest": "^29.5.14",
    "@types/semver": "^7.5.8",
    "@vitest/browser": "^3.0.8",
    "@vitest/coverage-v8": "^3.0.8",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "esbuild": "^0.25.1",
    "glob": "^11.0.1",
    "playwright": "^1.51.0",
    "tsup": "8.0.2",
    "turbo": "^2.4.4",
    "typescript": "^5.7.3",
    "vitest": "^3.0.8"
  },
  "workspaces": [
    "apps/web",
    "apps/docs",
    "apps/admin",
    "apps/estimate-system",
    "packages/*",
    "packages/features/*",
    "tooling/*"
  ],
  "dependencies": {
    "@google/generative-ai": "^0.24.0",
    "@nextui-org/spinner": "^2.2.6",
    "@octokit/graphql": "^8.2.1",
    "@react-pdf/renderer": "^4.3.0",
    "html-to-image": "^1.11.13",
    "react-intersection-observer": "^9.16.0"
  }
}
