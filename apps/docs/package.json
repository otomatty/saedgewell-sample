{
  "name": "docs",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "analyze": "ANALYZE=true bun run build",
    "build": "bun run with-env next build && bun run build:search",
    "build:vercel": "bun run copy-docs && bun run generate-doc-data && bun run with-env next build",
    "build:test": "NODE_ENV=test bun run with-env:test next build",
    "clean": "git clean -xdf .next .turbo node_modules",
    "dev": "bun run with-env next dev --turbo --port 7511 | pino-pretty -c",
    "lint": "biome lint .",
    "format": "biome format .",
    "start": "bun run with-env next start",
    "start:test": "NODE_ENV=test bun run with-env:test next start",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ./.env.local --",
    "with-env:test": "dotenv -e ./.env.test --",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "build:search": "bun run scripts/build-search-index.ts",
    "update:journal": "bash scripts/update-all-journals.sh",
    "update:journal:entries": "bun run scripts/update-journal-entries.ts",
    "update:journal:commits": "bun run scripts/update-journal-commits.ts --user otomatty",
    "create:journal:dirs": "bun run scripts/create-journal-dirs.ts",
    "create:journal:entry": "bun run scripts/create-journal-entry.ts",
    "find:duplicate:commits": "ts-node -T scripts/find-duplicate-commits.ts",
    "copy-docs": "node scripts/copy-docs.js",
    "generate-doc-data": "node scripts/generate-doc-data.js"
  },
  "dependencies": {
    "@braintree/sanitize-url": "^7.1.1",
    "@edge-csrf/nextjs": "2.5.3-cloudflare-rc1",
    "@google/generative-ai": "^0.24.0",
    "@heroicons/react": "2.2.0",
    "@hookform/resolvers": "4.1.3",
    "@kit/accounts": "workspace:*",
    "@kit/auth": "workspace:*",
    "@kit/i18n": "workspace:*",
    "@kit/next": "workspace:*",
    "@kit/supabase": "workspace:*",
    "@kit/ui": "workspace:*",
    "@makerkit/data-loader-supabase-core": "0.0.8",
    "@makerkit/data-loader-supabase-nextjs": "1.2.3",
